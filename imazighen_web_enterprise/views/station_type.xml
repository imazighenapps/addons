
<odoo>
  <data>

    <record model="ir.ui.view" id="station_type_tree_view">
      <field name="name">station.type.tree.view</field>
      <field name="model">station.type</field>
      <field name="arch" type="xml">
        <tree string="Type de gare" editable="top">
            <field name="name"/>
        </tree>
      </field>
    </record>


    <record model="ir.actions.act_window" id="station_type_action_window">
      <field name="name">Type de gare</field>
      <field name="res_model">station.type</field>
      <field name="view_mode">tree</field>
    </record>


  </data>
</odoo>

