<?xml version="1.0" encoding="utf-8"?>
<odoo>

<!-- AIPE -->
    <record id="module_category_fs_dashboard" model="ir.module.category">
        <field name="name">FS Dashboard</field>
        <field name="description">FS Dashboard Access group</field>
    </record>

    <record id="group_user" model="res.groups">
        <field name="name">user</field>
        <field name="category_id" ref="module_category_fs_dashboard"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_admin" model="res.groups">
        <field name="name">Admin</field>
        <field name="category_id" ref="module_category_fs_dashboard"/>
        <field name="implied_ids" eval="[(4, ref('fs_dashboard_access_group.group_user'))]"/>
    </record>


    </odoo>